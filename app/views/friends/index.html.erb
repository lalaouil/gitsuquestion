<p id="notice"><%= notice %></p>

<h1>Friends</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Zipcode</th>
      <th>Name</th>
      <th>Age</th>
      <th>Min perferr age</th>
      <th>Max pref age</th>
      <th>Gender</th>
      <th>Gender pref</th>
      <th>Support</th>
      <th>Friend level</th>
      <th>Look for</th>
      <th>Languauage</th>
      <th>Scheduled</th>
      <th>Look friend</th>
      <th>Interests</th>
      <th>Student</th>
      <th>Job</th>
      <th>Daily routine</th>
      <th>Triggers</th>
      <th>Description</th>
      <th>Clarify</th>
      <th>Forget something</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @friends.each do |friend| %>
      <tr>
        <td><%= friend.email %></td>
        <td><%= friend.zipcode %></td>
        <td><%= friend.name %></td>
        <td><%= friend.age %></td>
        <td><%= friend.min_perferr_age %></td>
        <td><%= friend.max_pref_age %></td>
        <td><%= friend.gender %></td>
        <td><%= friend.gender_pref %></td>
        <td><%= friend.support %></td>
        <td><%= friend.friend_level %></td>
        <td><%= friend.look_for %></td>
        <td><%= friend.languauage %></td>
        <td><%= friend.scheduled %></td>
        <td><%= friend.look_friend %></td>
        <td><%= friend.interests %></td>
        <td><%= friend.student %></td>
        <td><%= friend.job %></td>
        <td><%= friend.daily_routine %></td>
        <td><%= friend.triggers %></td>
        <td><%= friend.description %></td>
        <td><%= friend.clarify %></td>
        <td><%= friend.forget_something %></td>
        <td><%= link_to 'Show', friend %></td>
        <td><%= link_to 'Edit', edit_friend_path(friend) %></td>
        <td><%= link_to 'Destroy', friend, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Friend', new_friend_path %>
